class Solution {
    public boolean isPalindrome(int x) {
      
        if(x<0)
          return false;
        else
        { int t=x;
          int   p=0;
          while(x>0){
            int rem = x%10;
            p= p*10 + rem ;
             x=x/10 ;
          }
          if(p==t)
             return true;
        else
        return false;
        }
    }
}
    
